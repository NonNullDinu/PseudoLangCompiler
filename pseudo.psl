int n
int p
file_stream fout
int i
int x
int k
pointer arr
file_stream fin
begin
	allocate int, arr, 100
	open file fin, "pseudo.in", read only
	read file fin, n
	k <- 1
	for i <- 1, n do
	begin
		read file fin, x
		prime x, p
		if(p == true) then
		begin
			arr[k] <- x
			k <- k + 1
		end
	end
	write "found ", k-1, " primes", "\n"
	sort arr+1, k-1
	close file fin
	open file fout, "pseudo.maxdiff.out", write only, 744
	for i <- 1, k-1 do
	begin
		write file fout, arr[i], " "
	end
	close file fout
end
exit 0