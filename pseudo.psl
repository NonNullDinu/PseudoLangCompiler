int n
int p
int i
int x
int k
pointer arr
file_stream f
begin
	allocate int, arr, 100
	open file f, "pseudo.in", read only
	read file f, n
	k <- 1
	for i <- 1, n do
	begin
		read file f, x
		prime x, p
		if(p == true) then
		begin
			arr[k] <- x
			k <- k + 1
		end
	end
	close file f
	k <- k-1
	reverse_sort arr+1, k
	open file f, "pseudo.out", write only, 744
	for i <- 1, k do
	begin
		write file f, arr[i], " "
	end
	close file f
end
exit 0