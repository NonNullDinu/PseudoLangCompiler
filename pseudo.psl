int n
int p
file_stream fout
int prev
int dif
int difmax <- 0
int difmaxn
int difmaxprev
int true <- 1
int false <- 0
begin
	prev <- 2
	n <- 3
	while (!(n > 1000000))
	do begin
		prime n, p
		if(p)
		then begin
			dif <- n - prev
			if(dif > difmax)
			then begin
				difmax <- dif
				difmaxn <- n
				difmaxprev <- prev
			end
			prev <- n
		end
		n <- n + 2
	end
	write difmax, "\n"
	open file fout, "pseudo.maxdiff.out", write only, 744
	write file fout, difmax, "\n", difmaxprev, "\n", difmaxn
	close file fout
end
exit 0