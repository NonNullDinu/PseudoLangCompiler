begin
    int a
    file_stream fl
    file_stream out
    int i
    int n
    open file fl, "pseudo.in", read only
    open file out, "pseudo.out", write only, 744
    read file fl, n
    for i <- 1, n do
    begin
        read file fl, a
        write file out, a, "\n"
    end
    close file fl
    close file out
end
exit