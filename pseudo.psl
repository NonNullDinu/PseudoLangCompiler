declare n, i of type int
declare f of type file_stream
declare vec of type array[int]
begin
	open ("pseudo.in", read only) as file f
	read n from file f
	allocate vec, 100
	for i <- 1, n do
	begin
		read vec[i] from file f
	end
	close file f
	i <- n
	while (i > 0) do
	begin
		write (3 * i + vec[i-1])* vec[i] + i * 5, " "
		i <- i - 1
	end
end
exit